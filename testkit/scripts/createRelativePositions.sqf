_createRelativePositions = {
	private ["_missionObjects","_object","_position","_direction","_z","_array"];
	_missionObjects = _this;
	_posCenter = (_missionObjects select 0) select 1;
	_centerX = _posCenter select 0;
	_centerY = _posCenter select 1;
	_array = [];
	
	{
		_object = _x select 0;
		_position = _x select 1;
		//if (count _x > 2) then {_direction = _x select 2;};
		
		//_z = if (count _position > 2) then {_position select 2;} else {0;};
		_z = 0; // All objects should spawn on the ground in a WAI or DZMS mission.
		
		if (_foreachIndex == 0) then { // position of first object has no x,y offsets
			_pos = [0,0,_z];
			if (count _x > 2) then {
				_array set [count _array, [_object, _pos, (_x select 2)]];
			} else {
				_array set [count _array, [_object, _pos]];
			};
		} else {
			_pos = [(_position select 0) - _centerX, (_position select 1) - _centerY, _z];
			if (count _x > 2) then {
				_array set [count _array, [_object, _pos, (_x select 2)]];
			} else {
				_array set [count _array, [_object, _pos]];
			};
		};
		
	} forEach _missionObjects;
	
	diag_log text "[[";
	{
		if (_foreachIndex == (count _array - 1)) then {
			diag_log text format ["	%1",_x];
		} else {
			diag_log text format ["	%1,",_x];
		};
	} forEach _array;
	diag_log text "],_position,_mission] call wai_spawnObjects;";
};


